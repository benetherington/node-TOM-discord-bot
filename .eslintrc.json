{
    "extends": "eslint:recommended",
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["warn", {
            "tabWidth": 4,
            "printWidth" : 120,
            "singleQuote": true,
            "trailingComma": "all",
            "bracketSpacing": false,
            "jsxBracketSameLine": true
        }]
    }
}
